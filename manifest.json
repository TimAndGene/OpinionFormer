{
    "name": "Like blocker",
    "description": "Obscures the number of likes or views on a page so you can form your own opinion",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "browsingData"
    ],
    "background": {
        "scripts": ["obscure.js"],
        "persistent": false
    },
    "browser_action": {
        "default_title": "Obscure likes or views"
    },
    "manifest_version": 2
}