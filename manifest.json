{
    "name": "Like blocker",
    "description": "Obscures the number of likes or views on a page so you can form your own opinion",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": ["http://www.youtube.com/*"],
            "css": ["youtubeobscure.css"],
            "js": ["youtubeobscure.js"]
        }
    ]
}